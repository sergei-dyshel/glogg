{
    "clang-format.language.c.style": "file",
    "clang-format.language.cpp.style": "file",
    "dash.languageIdToDocsetMap.cpp": "qt5",
    "ccls.index.blacklist": ["yaml-cpp", "googletest", "googlemock", "autogen"],
    "ccls.launch.command": "q-lang-server",
    "ccls.launch.args": ["--ccls", "--filtered"],
    "clangd.path": "q-lang-server",
    "clangd.arguments": [
        "--clangd",
        "--filtered"
    ],
    "yaml.schemas": {
        "./colors.schema.json": "*.glogg-colors.yaml",
        "./syntax.schema.json": "*.glogg-syntax.yaml"
    },
    "files.associations": {
        "tasks.json": "jsonc",
        "*.json": "json"
    },
    "workbench.colorTheme": "Solarized Dark",
    "cmake.configureOnOpen": false
}
